{
  "$schema": "https://json.schemastore.org/swcrc",
  "env": {
    "targets": {
      "node": "16"
    }
  },
  "exclude": ["^.+\\.test\\.(t|j)sx?$"],
  "jsc": {
    "parser": {
      "syntax": "typescript"
    },
    "paths": {
      "src/*": ["src/*"]
    }
  },
  "minify": true,
  "module": {
    "type": "commonjs"
  }
}
