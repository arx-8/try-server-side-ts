{
  "name": "try-server-side-ts",
  "packageManager": "yarn@3.2.3",
  "private": true,
  "scripts": {
    "build": "node ./scripts/production.js",
    "dev:start": "nodemon --watch dist --ext js --exec 'node --inspect dist/index.js'",
    "dev:watch": "node ./scripts/dev.js",
    "dev": "(yarn dev:watch) & sleep 3 && yarn dev:start",
    "lint-dry": "run-p --continue-on-error 'lint:*'",
    "lint:tsc": "tsc --noEmit",
    "test-ci": "jest --coverage",
    "test": "jest --watch"
  },
  "devDependencies": {
    "@types/express": "4.17.14",
    "@types/jest": "29.0.3",
    "@types/lodash": "4.14.185",
    "@types/node": "18.7.18",
    "esbuild": "0.15.9",
    "esbuild-jest": "0.5.0",
    "jest": "29.0.3",
    "nodemon": "2.0.20",
    "npm-run-all": "4.1.5",
    "typescript": "4.8.3"
  },
  "dependencies": {
    "dotenv": "16.0.3",
    "express": "4.18.1",
    "lodash": "4.17.21",
    "tsd": "0.24.1"
  }
}
