{
  "name": "try-server-side-ts",
  "packageManager": "yarn@3.2.3",
  "private": true,
  "scripts": {
    "build": "node ./scripts/production.js",
    "dev:start": "nodemon --watch dist --ext js --exec 'node --inspect dist/index.js'",
    "dev:watch": "node ./scripts/dev.js",
    "dev": "(yarn dev:watch) & sleep 3 && yarn dev:start",
    "eslint-interactive": "eslint-interactive ./src",
    "eslint-print-config": "eslint --print-config src/index.ts > misc/eslint-print-config.json",
    "lint-dry:eslint": "eslint -c .eslintrc.js --max-warnings=0 .",
    "lint-dry:format": "prettier --check .",
    "lint-dry": "run-p --continue-on-error 'lint-dry:*' lint:tsc",
    "lint:eslint": "eslint -c .eslintrc.js --fix .",
    "lint:format": "prettier --write .",
    "lint:tsc": "tsc --noEmit",
    "lint": "run-p --continue-on-error 'lint:*'",
    "test-ci": "jest --coverage",
    "test": "jest --watch"
  },
  "devDependencies": {
    "@types/eslint": "8.4.6",
    "@types/express": "4.17.14",
    "@types/jest": "29.0.3",
    "@types/lodash": "4.14.185",
    "@types/node": "18.7.18",
    "@types/prettier": "2.7.1",
    "@typescript-eslint/eslint-plugin": "5.39.0",
    "@typescript-eslint/parser": "5.39.0",
    "esbuild": "0.15.9",
    "esbuild-jest": "0.5.0",
    "eslint": "8.24.0",
    "eslint-config-prettier": "8.5.0",
    "eslint-config-standard": "17.0.0",
    "eslint-interactive": "10.2.0",
    "eslint-plugin-import": "2.26.0",
    "eslint-plugin-jest": "27.1.0",
    "eslint-plugin-n": "15.3.0",
    "eslint-plugin-prefer-arrow-functions": "3.1.4",
    "eslint-plugin-promise": "6.0.1",
    "eslint-plugin-sort-destructure-keys": "1.4.0",
    "eslint-plugin-sort-keys-fix": "1.1.2",
    "eslint-plugin-typescript-sort-keys": "2.1.0",
    "jest": "29.0.3",
    "nodemon": "2.0.20",
    "npm-run-all": "4.1.5",
    "prettier": "2.7.1",
    "prettier-plugin-organize-imports": "3.1.1",
    "typescript": "4.8.3"
  },
  "dependencies": {
    "dotenv": "16.0.3",
    "express": "4.18.1",
    "lodash": "4.17.21",
    "mysql2": "2.3.3",
    "pg-hstore": "2.3.4",
    "sequelize": "6.23.2",
    "tsd": "0.24.1"
  }
}
