{
  "name": "try-server-side-ts",
  "packageManager": "yarn@3.2.3",
  "private": true,
  "scripts": {
    "build:cleanup": "rm -rf dist",
    "build": "yarn build:cleanup && swc src -d dist --config-file configs/.swcrc.production",
    "dev:build": "yarn build:cleanup && swc src -d dist --config-file configs/.swcrc.dev",
    "dev:start": "nodemon --watch dist --ext js --exec 'node dist/index.js'",
    "dev": "yarn dev:build --watch & sleep 3 && yarn dev:start",
    "lint-dry": "run-p --continue-on-error 'lint:*'",
    "lint:tsc": "tsc --noEmit",
    "test-ci": "jest --coverage",
    "test": "jest --watch"
  },
  "devDependencies": {
    "@swc/cli": "0.1.57",
    "@swc/core": "1.3.5",
    "@swc/jest": "0.2.23",
    "@types/express": "4.17.14",
    "@types/jest": "29.1.2",
    "@types/lodash": "4.14.186",
    "@types/node": "18.7.18",
    "jest": "29.1.2",
    "nodemon": "2.0.20",
    "npm-run-all": "4.1.5",
    "tsd": "0.24.1",
    "typescript": "4.8.3"
  },
  "dependencies": {
    "dotenv": "16.0.3",
    "express": "4.18.1",
    "lodash": "4.17.21"
  }
}
