{
  "name": "try-server-side-ts",
  "packageManager": "yarn@3.2.3",
  "private": true,
  "source": "src/index.ts",
  "main": "dist/index.js",
  "targets": {
    "main": {
      "context": "node",
      "includeNodeModules": true
    }
  },
  "scripts": {
    "build": "parcel build --no-source-maps",
    "dev:start": "nodemon --watch dist --ext js --exec 'node --inspect dist/index.js'",
    "dev:watch": "parcel watch",
    "dev": "(yarn dev:watch) & sleep 3 && yarn dev:start",
    "lint-dry": "run-p --continue-on-error 'lint:*'",
    "lint:tsc": "tsc --noEmit",
    "test": "echo No test yet."
  },
  "devDependencies": {
    "@types/express": "4.17.14",
    "@types/lodash": "4.14.185",
    "@types/node": "18.7.18",
    "nodemon": "2.0.20",
    "npm-run-all": "4.1.5",
    "parcel": "2.7.0",
    "typescript": "4.8.3"
  },
  "dependencies": {
    "express": "4.18.1",
    "lodash": "4.17.21"
  }
}
